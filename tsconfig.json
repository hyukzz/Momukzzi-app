{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "strict": true,
    "target": "es5",
  },
  "exclude": ["node_modules"],
  "typeRoots": ["./types"]
}
